{
	"manifest_version": 2,

	"name": "Vocab Grabber",
	"version": "0.1",
	"description": "Stores the words you looked up on Google's dictionary",

	"permissions": ["activeTab", "storage"],

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},

	"browser_action": {
		"default_popup": "popup.html"
	},

	"content_scripts": [{
		"matches": ["https://www.google.com/search*"],
		"js": ["content.js"]
	}]
}


